{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/participation",
      "destination": "/participation.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
